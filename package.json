{
  "name": "gemini-deep-research-oss",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "db:generate": "drizzle-kit generate",
    "migrate:remote": "npm run db:generate && wrangler d1 migrations apply DB --remote",
    "deploy": "npm run build && npm run migrate:remote && wrangler deploy"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/genai": "^1.38.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@tailwindcss/vite": "^4.1.18",
    "better-sqlite3": "^12.6.2",
    "drizzle-kit": "^0.31.9",
    "drizzle-orm": "^0.45.1",
    "moment": "^2.30.1",
    "radash": "^12.1.1",
    "react": "^19.2.4",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-router": "^7.13.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwindcss": "^4.1.18",
    "uuid": "^11.1.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.22.1",
    "@cloudflare/workers-types": "^4.20260129.0",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "wrangler": "^4.61.1"
  },
  "engines": {
    "node": ">=20.0.0 <23.0.0"
  }
}
